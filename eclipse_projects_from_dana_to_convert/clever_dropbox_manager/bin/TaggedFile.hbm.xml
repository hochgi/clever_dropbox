<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
 
  <hibernate-mapping>
      <class name="dataBase.TaggedFile" table="tagged_files" >
    
<composite-id>
<key-property name="tag" column="tag" type="java.lang.String" length="100"/>
<key-property name="file" column="file" type="java.lang.String" length="100"/>
</composite-id>

      </class>
  </hibernate-mapping>


